{
   "background": {
      "scripts": [ "src/background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/inject.css" ],
      "js": [ "src/inject/inject.js" ],
      "matches": [ "http://www.youtube.com/*", "https://www.youtube.com/*" ]
   } ],
   "description": "Control speed of the youtube playback using keyboard. Press '+' to increase and '-' to decrease the playback speed.",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Youtube Playback Speed Control",
   "options_page": "src/option/options.html",
   "page_action": {
      "default_icon": {
         "19": "icons/icon19.png"
      },
      "default_title": "Control speed of the youtube playback using keyboard."
   },
   "permissions": [ "tabs", "activeTab", "storage", "webRequest" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.5"
}
